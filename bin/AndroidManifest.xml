<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="final_project.push"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk
		android:minSdkVersion="11"
		android:targetSdkVersion="17" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >
		<activity
			android:name="final_project.push.Push"
			android:label="@string/title_activity_push" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="final_project.push.About"
			android:label="@string/about_title"
			android:theme="@android:style/Theme.Dialog" >
		</activity>
		<activity
			android:name="final_project.push.Start"
			android:label="@string/title_activity_start" >
		</activity>
		<activity
			android:name="final_project.push.Stats"
			android:label="@string/title_activity_stats"
			android:theme="@android:style/Theme.Dialog" >
		</activity>
		<activity
			android:name="final_project.push.Settings"
			android:label="@string/title_activity_settings" >
		</activity>
		<activity
			android:name="final_project.push.Timer"
			android:label="@string/title_activity_timer" >
		</activity>
		<activity
			android:name="final_project.push.SaveWorkout"
			android:label="@string/title_activity_save_workout"
			android:theme="@android:style/Theme.Dialog" >
		</activity>
		<activity
			android:name="final_project.push.Congrats"
			android:label="@string/title_activity_congrats"
			android:theme="@android:style/Theme.Dialog" >
		</activity>
		<activity
			android:name="final_project.push.ClearData"
			android:label="@string/title_activity_clear_data"
			android:theme="@android:style/Theme.Dialog" >
		</activity>
	</application>

</manifest>